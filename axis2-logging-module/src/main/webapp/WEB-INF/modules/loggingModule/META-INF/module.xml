<module name="logging" class="fr.cnrs.mdm.referentiel.ws.LoggingModule">
   <InFlow>
        <handler name="InFlowLogHandler" class="fr.cnrs.mdm.referentiel.ws.LoggingHandler">
        <order phase="loggingPhase" />
        </handler>
   </InFlow>

   <OutFlow>
        <handler name="OutFlowLogHandler" class="fr.cnrs.mdm.referentiel.ws.LoggingHandler">
        <order phase="loggingPhase"/>
        </handler>
   </OutFlow>

   <OutFaultFlow>
        <handler name="FaultOutFlowLogHandle" class="fr.cnrs.mdm.referentiel.ws.LoggingHandler">
        <order phase="loggingPhase"/>
        </handler>
   </OutFaultFlow>

   <InFaultFlow>
        <handler name="FaultInFlowLogHandler" class="fr.cnrs.mdm.referentiel.ws.LoggingHandler">
        <order phase="loggingPhase"/>
        </handler>
   </InFaultFlow>
</module>